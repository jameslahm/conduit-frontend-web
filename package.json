{
	"name": "conduit-frontend-web",
	"version": "0.2.5",
	"private": true,
	"main": "public/electron.js",
	"repository": {
		"type": "git",
		"url": "https://github.com:jameslahm/conduit-frontend-web.git"
	},
	"description": "Conduit Electron",
	"author": {
		"name": "jameslahm",
		"email": "wa17@mails.tsinghua.edu.cn"
	},
	"dependencies": {
		"@apollo/react-hooks": "^3.1.5",
		"@material-ui/core": "^4.11.0",
		"@material-ui/icons": "^4.9.1",
		"@material-ui/lab": "^4.0.0-alpha.56",
		"@reach/router": "^1.3.4",
		"@testing-library/jest-dom": "^5.11.0",
		"@testing-library/react": "^9.3.2",
		"@testing-library/user-event": "^7.1.2",
		"apollo-cache-inmemory": "^1.6.6",
		"apollo-client": "^2.6.10",
		"apollo-link": "^1.2.14",
		"apollo-link-context": "^1.0.20",
		"apollo-link-http": "^1.5.17",
		"apollo-link-persisted-queries": "^0.2.2",
		"apollo-utilities": "^1.3.4",
		"date-fns": "^2.14.0",
		"electron-is-dev": "^1.2.0",
		"electron-updater": "^4.3.1",
		"graphql-tag": "^2.10.4",
		"markdown-it": "^11.0.0",
		"markdown-it-abbr": "^1.0.4",
		"markdown-it-emoji": "^1.4.0",
		"markdown-it-footnote": "^3.0.2",
		"markdown-it-mark": "^3.0.0",
		"markdown-it-sub": "^1.0.0",
		"markdown-it-sup": "^1.0.0",
		"notistack": "^0.9.17",
		"prismjs": "^1.20.0",
		"qs": "^6.9.4",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-markdown": "^4.3.1",
		"react-redux": "^7.2.0",
		"react-scripts": "3.4.1",
		"redux": "^4.0.5",
		"validator": "^13.1.1"
	},
	"homepage": "./",
	"scripts": {
		"electron-start": "concurrently \"wait-on http-get://localhost:3000 && electron .\" \"cross-env BROWSER=none yarn start\"",
		"start": "react-app-rewired start",
		"build": "react-app-rewired build",
		"test": "react-app-rewired test",
		"eject": "react-scripts eject",
		"postinstall": "electron-builder install-app-deps",
		"package": "yarn build && electron-builder --dir",
		"dist": "yarn build && electron-builder",
		"release": "yarn build && electron-builder -p always",
		"prebuild": "rimraf build dist",
		"generate": "apollo client:codegen --target typescript --watch"
	},
	"build": {
		"appId": "com.electron.material-conduit",
		"productName": "Conduit",
		"files": [
			"build/**/*",
			"node_modules/**/*"
		],
		"publish": {
			"provider": "github",
			"repo": "conduit-frontend-web",
			"owner": "jameslahm"
		},
		"win": {
			"icon": "build/icon.png"
		}
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	},
	"lint-staged": {
		"*.{js,ts,tsx,jsx,css}": [
			"prettier --write",
			"eslint --fix"
		]
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@commitlint/cli": "^9.0.1",
		"@commitlint/config-conventional": "^9.0.1",
		"@types/jest": "^26.0.4",
		"@types/markdown-it": "^10.0.1",
		"@types/markdown-it-emoji": "^1.4.0",
		"@types/node": "^14.0.22",
		"@types/prismjs": "^1.16.1",
		"@types/qs": "^6.9.3",
		"@types/reach__router": "^1.3.5",
		"@types/react": "^16.9.0",
		"@types/react-dom": "^16.9.0",
		"@types/react-redux": "^7.1.9",
		"@types/validator": "^13.1.0",
		"apollo": "^2.29.1",
		"babel-plugin-prismjs": "^2.0.1",
		"concurrently": "^5.2.0",
		"cross-env": "^7.0.2",
		"customize-cra": "^1.0.0",
		"dotenv": "^8.2.0",
		"electron": "^9.1.0",
		"electron-builder": "^22.7.0",
		"husky": "^4.2.5",
		"lint-staged": "^10.2.11",
		"prettier": "^2.0.5",
		"react-app-rewired": "^2.1.6",
		"rimraf": "^3.0.2",
		"typescript": "~3.9.6",
		"wait-on": "^5.1.0"
	}
}
